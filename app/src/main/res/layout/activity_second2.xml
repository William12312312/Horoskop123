<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">





    <TextView
        android:id="@+id/dateRange"
        android:layout_width="179dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="5dp"
        android:text="Daterange"
        app:layout_constraintBottom_toTopOf="@+id/currentDate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/currentDate"
        android:layout_width="179dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="5dp"
        android:text="current date"
        app:layout_constraintBottom_toTopOf="@+id/description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dateRange" />

    <TextView
        android:id="@+id/description"
        android:layout_width="179dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="5dp"
        android:text="Description"
        app:layout_constraintBottom_toTopOf="@+id/compatibility"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/currentDate" />

    <TextView
        android:id="@+id/compatibility"
        android:layout_width="179dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="5dp"
        android:text="Compatibility"
        app:layout_constraintBottom_toTopOf="@+id/mood"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/description" />

    <TextView
        android:id="@+id/mood"
        android:layout_width="179dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="5dp"
        android:text="Mood"
        app:layout_constraintBottom_toTopOf="@+id/color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/compatibility" />

    <TextView
        android:id="@+id/color"
        android:layout_width="179dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="10dp"
        android:text="Color"
        app:layout_constraintBottom_toTopOf="@+id/luckyNumber"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mood" />

    <TextView
        android:id="@+id/luckyNumber"
        android:layout_width="179dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="10dp"
        android:text="Lucky Number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/color" />

    <TextView
        android:id="@+id/luckyTime"
        android:layout_width="179dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="175dp"
        android:text="Lucky Time"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/color"
        app:layout_constraintVertical_bias="1.0" />

</LinearLayout>